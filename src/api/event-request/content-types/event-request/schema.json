{
  "kind": "collectionType",
  "collectionName": "event_requests",
  "info": {
    "singularName": "event-request",
    "pluralName": "event-requests",
    "displayName": "Event Request"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "clientName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "eventDate": {
      "type": "datetime"
    },
    "eventLocation": {
      "type": "string"
    },
    "guestCount": {
      "type": "integer"
    },
    "eventType": {
      "type": "enumeration",
      "enum": [
        "wedding",
        "corporate",
        "birthday",
        "other"
      ]
    },
    "Decimal": {
      "type": "decimal"
    },
    "description": {
      "type": "blocks"
    }
  }
}
